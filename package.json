{
  "name": "toolbot-core-experiment",
  "version": "0.10.0-6",
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "browser": "lib/browser.js",
  "files": [
    "bin",
    "lib",
    "express.js",
    "koa.js",
    "micro.js",
    "restify.js"
  ],
  "bin": {
    "bottender": "bin/cli.js"
  },
  "scripts": {
    "build": "npm run clean && babel src -d lib --ignore __tests__",
    "clean": "rimraf lib",
    "check": "flow check",
    "lint": "eslint bin src scripts",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged",
    "precommit": "lint-staged",
    "test": "npm run lint:fix && npm run check && npm run testonly",
    "testonly": "jest",
    "testonly:cov": "jest --coverage --runInBand --forceExit",
    "testonly:watch": "jest --watch",
    "preversion": "npm test",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.9.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-yoctol": "^0.6.0",
    "eslint": "^4.4.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-yoctol-base": "^0.13.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.2.0",
    "flow-bin": "^0.53.1",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "jest-junit": "^2.1.0",
    "lint-staged": "^4.0.3",
    "ngrok": "^2.2.17",
    "prettier": "^1.5.3",
    "rimraf": "^2.6.1",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "@yoctol/static": "^0.0.2",
    "axios": "^0.16.2",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.17.2",
    "chalk": "^2.1.0",
    "child_process": "^1.0.2",
    "cli-table2": "^0.2.0",
    "commander": "^2.11.0",
    "cross-spawn": "^5.1.0",
    "debug": "^3.0.0",
    "delay": "^2.0.0",
    "didyoumean": "^1.2.1",
    "express": "^4.15.4",
    "file-type": "^6.1.0",
    "fs-extra": "^4.0.1",
    "glob": "^7.1.2",
    "hasha": "^3.0.0",
    "inquirer": "^3.2.2",
    "invariant": "^2.2.2",
    "ioredis": "^3.1.4",
    "is-image": "^2.0.0",
    "jfs": "^0.2.6",
    "jsonfile": "^3.0.1",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compose": "^4.0.0",
    "koa-router": "^7.2.1",
    "lodash": "^4.17.4",
    "messaging-api-line": "^0.2.4",
    "messaging-api-messenger": "^0.2.6",
    "messaging-api-slack": "^0.2.6",
    "messaging-api-telegram": "^0.2.7",
    "micro": "^8.0.3",
    "mongodb": "^2.2.31",
    "pascal-case": "^2.0.1",
    "quick-lru": "^1.1.0",
    "random-item": "^1.0.0",
    "readline": "^1.3.0",
    "restify": "^5.2.0",
    "shortid": "^2.2.8",
    "thenify": "^3.3.0",
    "validate-npm-package-name": "^3.0.0",
    "warning": "^3.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/",
      "Dangerous.*\\.js",
      "dangerous.*\\.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "setupFiles": [
      "./test/setupJest.js"
    ],
    "testResultsProcessor": "./node_modules/jest-junit",
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "testEnvironment": "node",
    "timers": "fake",
    "resetModules": true,
    "resetMocks": true
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  }
}
