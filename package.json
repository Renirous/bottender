{
  "name": "bottender",
  "description": "Make Bots Your Way, Fast and Flexible",
  "license": "MIT",
  "homepage": "https://bottender.js.org/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yoctol/bottender.git"
  },
  "version": "0.16.0-rc.1",
  "main": "lib/index.js",
  "browser": "lib/browser.js",
  "bin": {
    "bottender": "bin/cli.js",
    "btd": "bin/cli.js"
  },
  "files": [
    "bin",
    "express.js",
    "koa.js",
    "lib",
    "micro.js",
    "restify.js",
    "test-utils.js",
    "utils.js"
  ],
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production babel src -d lib --copy-files",
    "check": "flow check",
    "clean": "rimraf lib",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged",
    "prepublishOnly": "npm run build",
    "test": "npm run lint && npm run check && npm run testonly",
    "testonly": "jest --detectOpenHandles --forceExit",
    "testonly:cov": "jest --coverage --runInBand --detectOpenHandles --forceExit",
    "testonly:watch": "jest --watch",
    "preversion": "npm test"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.1",
    "@slack/rtm-api": "^5.0.2",
    "arg": "^4.1.1",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "camel-case": "^3.0.0",
    "chalk": "~2.4.2",
    "child_process": "^1.0.2",
    "cli-table3": "^0.5.1",
    "cross-spawn": "^6.0.5",
    "date-fns": "^2.0.1",
    "debug": "^4.1.1",
    "deep-object-diff": "^1.1.0",
    "delay": "^4.3.0",
    "express": "^4.17.1",
    "figures": "^3.0.0",
    "file-type": "^12.2.0",
    "fs-extra": "^8.1.0",
    "hasha": "^5.0.0",
    "import-fresh": "^3.1.0",
    "inquirer": "^7.0.0",
    "invariant": "^2.2.4",
    "ioredis": "^4.14.0",
    "jfs": "^0.3.0",
    "jsonfile": "^5.0.0",
    "koa": "^2.8.1",
    "koa-bodyparser": "^4.2.1",
    "koa-compose": "^4.1.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.15",
    "lru-cache": "^5.1.1",
    "messaging-api-line": "^0.8.1",
    "messaging-api-messenger": "^0.8.1",
    "messaging-api-slack": "^0.8.1",
    "messaging-api-telegram": "^0.8.1",
    "messaging-api-viber": "^0.8.1",
    "messenger-batch": "^0.3.0",
    "micro": "^9.3.4",
    "minimist": "^1.2.0",
    "mongodb": "^3.3.1",
    "p-map": "^3.0.0",
    "p-props": "^3.0.1",
    "pascal-case": "^2.0.1",
    "pkg-dir": "^4.2.0",
    "prompt-confirm": "^2.0.4",
    "read-chunk": "^3.2.0",
    "readline": "^1.3.0",
    "recursive-readdir": "^2.2.2",
    "restify": "^8.4.0",
    "shortid": "^2.2.14",
    "stringify-object": "^3.3.0",
    "thenify": "^3.3.0",
    "update-notifier": "^3.0.1",
    "urlencoded-body-parser": "^3.0.0",
    "validate-npm-package-name": "^3.0.0",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "axios-mock-adapter": "^1.17.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-lodash": "^3.3.4",
    "cross-env": "^5.2.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-config-yoctol-base": "^0.21.0",
    "eslint-plugin-flowtype": "^4.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.4.0",
    "flow-bin": "^0.94.0",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "jest-create-mock-instance": "^1.1.0",
    "jest-junit": "^8.0.0",
    "lint-staged": "^9.2.5",
    "ngrok": "^3.2.5",
    "once": "^1.4.0",
    "prettier": "^1.18.2",
    "prettier-package-json": "^2.1.0",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2"
  },
  "keywords": [
    "bot",
    "chatbot",
    "framework",
    "line",
    "messaging",
    "messenger",
    "slack",
    "telegram",
    "viber"
  ],
  "engines": {
    "node": ">=7.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "setupFiles": [
      "./test/setupJest.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/examples/",
      "/lib/",
      "src/cli/providers/sh/test.js"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "testEnvironment": "node",
    "timers": "fake",
    "resetModules": true,
    "resetMocks": true
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  }
}
