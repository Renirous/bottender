    "underscore.string": "^3.3.4",
    "warning": "^3.0.0",
    "yeoman-environment": "^1.6.6",
    "yeoman-generator": "^1.1.1"
  },
  "jest": {
    "moduleNameMapper": {
      "^test/(.*)": "<rootDir>/test/helpers/$1.js"
    },
    "collectCoverageFrom": [
      "src/cli/**/*.js",
      "src/core/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/",
      "Dangerous.*\\.js",
      "dangerous.*\\.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 87,
        "functions": 87,
        "lines": 87,
        "statements": 87
      }
    },
    "testResultsProcessor": "./node_modules/jest-junit",
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  }
}
